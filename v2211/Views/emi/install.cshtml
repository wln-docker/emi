@{ Layout = "../_all.cshtml";}
@section head{
    <style type="text/css">
        * {margin:0px;padding:0px;}
        *,:after,:before {box-sizing:border-box;}
        html,body {width:100%;height:100%;display:table;}
        body {position:fixed;z-index:0;top:0;right:0;bottom:0;left:0;background:#2f4050;text-align:center;font-family:"Arial","Microsoft YaHei","黑体","宋体",sans-serif;}
        .main {width:100%;height:100%;display:table-cell;text-align:center;vertical-align:middle;}
        .container {width:768px;height:450px;top:43%;left:50%;position:absolute;-webkit-transform:translate(-50%,-50%);-moz-transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);-o-transform:translate(-50%,-50%);transform:translate(-50%,-50%);overflow:hidden;box-shadow:0 0 15px rgb(0 138 233 / 15%);}
        .login-left {color:#fff;float:left;width:272px;height:450px;text-align:left;border-radius:10px 0 0 10px;background:url(data:image/png;base64,) center center no-repeat;background-size:cover;position:relative;user-select:none;padding-top:80px;padding-left:24px;}
        .login-left .login-title {font-size:28px;letter-spacing:9px;}
        .login-left .login-system {font-size:21px;max-width:236px;margin-top:10px;letter-spacing:3px;}
        .login-left .login-setups {height:168px;margin-top:30px;margin-left:30px;}
        .login-left .login-setup {font-size:21px;bottom:18px;right:18px;position:absolute;}
        .login-main {float:left;width:496px;height:450px;padding:50px 60px;background-color:#fff;position:relative;border-radius:0 10px 10px 0;}
        .login-tabs {display:flex;outline:none;text-align:left;margin-bottom:16px;border-bottom:1px solid #e8e8e8;justify-content:space-between;}
        .login-tabs-nav {bottom:-1px;color:#1890ff;position:relative;font-size:20px;font-weight:500;border-bottom:2px solid #1890ffff;padding:13px 12px 9px;cursor:pointer;}
        .login-form-item {box-sizing:border-box;padding:0;font-size:14px;line-height:1.5;font-feature-settings:"tnum";margin-bottom:18px;vertical-align:top;}
        .login-form-item.msg{color:#ff5722;text-align:left;margin-bottom:12px;margin-top:-15px}
        .login-form-affix {box-sizing:border-box;margin:0;padding:0;color:rgba(0,0,0,.65);font-size:14px;line-height:1.5;list-style:none;font-feature-settings:"tnum";position:relative;}
        .login-form-prefix,.login-form-suffix {position:absolute;top:50%;z-index:2;font-size:18px;display:flex;-webkit-box-align:center;align-items:center;color:rgba(0,0,0,.65);line-height:0;-webkit-transform:translateY(-50%);transform:translateY(-50%);}
        .login-form-prefix {left:9px;margin-right:10px;}
        .login-form-suffix {right:9px;}
        .login-form-input {box-sizing:border-box;margin:0;list-style:none;font-feature-settings:"tnum";position:relative;text-align:left;display:inline-block;width:100%;height:32px;padding:4px 11px;color:rgba(0,0,0,.65);font-size:14px;line-height:1.5;background-color:#fff;background-image:none;border:1px solid #d9d9d9;border-radius:3px;transition:all .3s;height:40px;padding:6px 11px;padding-left:30px;font-size:15px;color:#000000;}
        .login-form-input::placeholder {color:#cbcbcb;font-size:14px;}
        .el-button{width:100px;letter-spacing:3px}
        .el-step__head.is-finish{border-color:#ffffff;} .el-step__title.is-finish{color:#ffffff;}
        .el-step__head.is-process{color:#ffc107;border-color:#ffc107;} .el-step__title.is-process{color:#ffeb3b;}
    </style>
    <script type="text/javascript" src="@ViewBag.Assets/js/md5.js"></script>
}
@section body{
    <div class="main" id="app">
        <div class="container">
            <div class="login-left">
                <div class="login-title">欢迎使用</div>
                <div class="login-system">EMI系统初始化服务</div>
                <div class="login-setups">
                    <el-steps :active="setup-1" direction="vertical" align-center>
                        <el-step :title="item" v-for="item in setups"></el-step>
                    </el-steps>
                </div>
                <div class="login-setup">{{setup}}/{{setups.length}}</div>
            </div>
            <div class="login-main">
                <template v-if="setup === 1">
                    <div class="login-tabs">
                        <div class="login-tabs-nav">系统基础信息</div>
                    </div>
                    <div class="login-form">
                        <div class="login-form-item">
                            <span class="login-form-affix">
                                <i class="login-form-prefix el-icon-place"></i>
                                <input type="text" v-model="organ_name" placeholder="使用单位名称，如：XXX公司" maxlength="30" class="login-form-input" />
                            </span>
                        </div>
                        <div class="login-form-item msg" v-if="organ_name_msg">{{organ_name_msg}}</div>

                        <div class="login-form-item">
                            <span class="login-form-affix">
                                <i class="login-form-prefix el-icon-open"></i>
                                <input type="text" v-model="systemname" placeholder="业务系统名称，如：EMI业务管理系统" maxlength="20" class="login-form-input" />
                            </span>
                        </div>
                        <div class="login-form-item msg" v-if="systemname_msg">{{systemname_msg}}</div>

                        <div class="login-form-item">
                            <span class="login-form-affix">
                                <i class="login-form-prefix el-icon-key"></i>
                                <input type="text" v-model="systemtoken" placeholder="系统注册密钥（没有可不填，将自动生成）" maxlength="50" class="login-form-input" />
                            </span>
                        </div>
                        <div class="login-form-item msg" v-if="systemtoken_msg">{{systemtoken_msg}}</div>

                        <div class="login-form-item" style="text-align:right">
                            <el-button type="primary" size="small" v-on:click="toSetup2()">下一步</el-button>
                        </div>
                    </div>
                </template>
                <template v-if="setup === 2">
                    <div class="login-tabs">
                        <div class="login-tabs-nav">系统管理员</div>
                    </div>
                    <div class="login-form">
                        <div class="login-form-item">
                            <span class="login-form-affix">
                                <i class="login-form-prefix el-icon-user"></i>
                                <input type="text" v-model="super_username" placeholder="登录账号，如：super，长度为3~20个字符" maxlength="20" class="login-form-input" />
                            </span>
                        </div>
                        <div class="login-form-item msg" v-if="super_username_msg">{{super_username_msg}}</div>

                        <div class="login-form-item">
                            <span class="login-form-affix">
                                <i class="login-form-prefix el-icon-lock"></i>
                                <input type="password" v-model="super_password" placeholder="请设置此账号登录密码，长度为6~20个字符" maxlength="20" class="login-form-input" />
                            </span>
                        </div>
                        <div class="login-form-item msg" v-if="super_password_msg">{{super_password_msg}}</div>

                        <div class="login-form-item">
                            <span class="login-form-affix">
                                <i class="login-form-prefix el-icon-lock"></i>
                                <input type="password" v-model="super_repasswd" placeholder="再次输入账号登录密码，确保两次输入一致" maxlength="20" class="login-form-input" />
                            </span>
                        </div>
                        <div class="login-form-item msg" v-if="super_repasswd_msg">{{super_repasswd_msg}}</div>

                        <div class="login-form-item" style="text-align:right">
                            <el-button type="info" size="small" v-on:click="toSetup1()">上一步</el-button>
                            <el-button type="primary" size="small" v-on:click="toSetup3()">下一步</el-button>
                        </div>
                    </div>
                </template>
                <template v-if="setup === 3">
                    <div class="login-tabs">
                        <div class="login-tabs-nav">管理员设置</div>
                    </div>
                    <div class="login-form">
                        <div class="login-form-item">
                            <span class="login-form-affix">
                                <i class="login-form-prefix el-icon-user"></i>
                                <input type="text" v-model="admin_username" placeholder="登录账号，如：admin，长度为3~20个字符" maxlength="20" class="login-form-input" />
                            </span>
                        </div>
                        <div class="login-form-item msg" v-if="admin_username_msg">{{admin_username_msg}}</div>

                        <div class="login-form-item">
                            <span class="login-form-affix">
                                <i class="login-form-prefix el-icon-lock"></i>
                                <input type="password" v-model="admin_password" placeholder="请设置此账号登录密码，长度为6~20个字符" maxlength="20" class="login-form-input" />
                            </span>
                        </div>
                        <div class="login-form-item msg" v-if="admin_password_msg">{{admin_password_msg}}</div>

                        <div class="login-form-item">
                            <span class="login-form-affix">
                                <i class="login-form-prefix el-icon-lock"></i>
                                <input type="password" v-model="admin_repasswd" placeholder="再次输入账号登录密码，确保两次输入一致" maxlength="20" class="login-form-input" />
                            </span>
                        </div>
                        <div class="login-form-item msg" v-if="admin_repasswd_msg">{{admin_repasswd_msg}}</div>

                        <div class="login-form-item" style="text-align:right">
                            <el-button type="info" size="small" v-on:click="toSetup2()">上一步</el-button>
                            <el-button type="primary" size="small" v-on:click="toSetup4()">下一步</el-button>
                        </div>
                    </div>
                </template>
                <template v-if="setup === 4">
                    <div class="login-form" style="user-select:none">
                        <div style="color: #3cbdf7;font-size:18px;font-weight:bolder;line-height:3rem">
                            <font v-if="progress_status==100">系统初始化完成，感谢您的支持</font>
                            <font v-else-if="install_msg">系统初始化错误，请按提示检查</font>
                            <font v-else>系统正在执行初始化，请稍候</font>
                        </div>
                        <div style="padding:30px">
                            <el-progress type="circle" :status="progress_status" :percentage="progress_setup"></el-progress>
                        </div>
                        <div v-if="show_success">
                            <el-button type="primary" size="small" v-on:click="toFinish()" style="width:188px">开始使用</el-button>
                        </div>
                        <div style="color:#f44336;line-height:3rem" v-else-if="install_msg">
                            <div>{{install_msg}}</div>
                            <el-button type="info" size="small" v-on:click="toSetup1()">返回修改</el-button>
                        </div>
                    </div>
                </template>
            </div>
        </div>
    </div>
}
@section foot{
    <script type="text/javascript">
        document.title = "系统初始化"
        new Vue({
            el: '#app',
            data() {
                return {
                    setup: 1,
                    setups: ['系统基础信息', '系统管理员配置', '管理员配置', '业务数据初始化导入', '完成，返回登录'],
                    progress_setup: 0,
                    progress_status: '',
                    organ_name: '',
                    systemname: '',
                    systemtoken: '',
                    organ_name_msg: '',
                    systemname_msg: '',
                    systemtoken_msg: '',
                    super_username: '',
                    super_password: '',
                    super_repasswd: '',
                    super_username_msg: '',
                    super_password_msg: '',
                    super_repasswd_msg: '',
                    admin_username: '',
                    admin_password: '',
                    admin_repasswd: '',
                    admin_username_msg: '',
                    admin_password_msg: '',
                    admin_repasswd_msg: '',
                    show_success: false,
                    install_msg: ''
                }
            },
            methods: {
                clear() {
                    this.progress_setup = 0;
                    this.progress_status = '';
                    this.organ_name_msg = '';
                    this.systemname_msg = '';
                    this.systemtoken_msg = '';
                    this.super_username_msg = '';
                    this.super_password_msg = '';
                    this.super_repasswd_msg = '';
                    this.admin_username_msg = '';
                    this.admin_password_msg = '';
                    this.admin_repasswd_msg = '';
                    this.install_success = false;
                    this.install_msg = '';
                },
                Install() {
                    let obj = {}
                    obj.organ_name = this.organ_name;
                    obj.systemname = this.systemname;
                    obj.systemtoken = this.systemtoken;
                    obj.super_username = this.super_username;
                    obj.super_password = hex_md5(hex_md5(hex_md5(this.super_password).toUpperCase()).toUpperCase()).toUpperCase();
                    obj.admin_username = this.admin_username;
                    obj.admin_password = hex_md5(hex_md5(hex_md5(this.admin_password).toUpperCase()).toUpperCase()).toUpperCase();
                    console.log(obj)
                    this.progress_setup = 1;
                    setInterval(this.Percentage, 300);
                    post('/install/setup', obj).then(rlt => {
                        if (rlt.success) {
                            fetch('/install/finish?do=check', { method: 'GET' }).then(res => res.text()).then(res => {
                                setTimeout(() => { this.progress_setup = 100 }, 3000)
                                if (res == 'none') {
                                    setTimeout(() => { this.show_success = true }, 3500);
                                } else {
                                    setTimeout(this.toFinish, 4000)
                                }
                            })
                        } else {
                            setTimeout(() => {
                                this.install_msg = rlt.message
                                this.progress_status = 'warning';
                            }, 1500);
                        }
                    })
                },
                Percentage() {
                    if (this.progress_setup > 0 && this.progress_setup < 90 && !this.progress_status) {
                        this.progress_setup += Math.round(Math.random() * 10)
                    }
                },
                toSetup1() {
                    this.clear()
                    this.setup = 1;
                },
                toSetup2() {
                    this.clear()
                    let success = true
                    if (!this.organ_name) {
                        success = false
                        this.organ_name_msg = '使用单位名称未设置，请设置'
                    } else if (this.organ_name.length < 3 || this.organ_name.length > 30) {
                        success = false
                        this.organ_name_msg = '使用单位名称长度不能小于3或大于30'
                    }
                    if (this.organ_name && (this.organ_name.length < 3 || this.organ_name.length > 20)) {
                        success = false
                        this.organ_name_msg = '业务系统名称长度不能小于3或大于20'
                    }
                    if (this.systemname && (this.systemname.length < 3 || this.systemname.length > 20)) {
                        success = false
                        this.systemname_msg = '业务系统名称长度不能小于3或大于20'
                    }
                    if (this.systemtoken && (this.systemtoken.length < 20 || this.systemtoken.length > 50)) {
                        success = false
                        this.systemtoken_msg = '注册密钥长度不能小于20或大于50'
                    }
                    if (success) {
                        this.setup = 2
                    }
                },
                toSetup3() {
                    this.clear()
                    let success = true
                    if (!this.super_username) {
                        success = false
                        this.super_username_msg = '系统管理员账号未设置，请设置'
                    } else if (this.super_username.length < 3 || this.super_username.length > 20) {
                        success = false
                        this.super_username_msg = '系统管理员账号长度不能小于3或大于20'
                    }
                    if (!this.super_password) {
                        success = false
                        this.super_password_msg = '系统管理员密码未设置，请设置'
                    } else if (this.super_password.length < 6 || this.super_password.length > 20) {
                        success = false
                        this.super_password_msg = '密码长度不能小于6或大于20'
                    }
                    if (!this.super_repasswd) {
                        success = false
                        this.super_repasswd_msg = '请再次输入账号登录密码'
                    } else if (this.super_password != this.super_repasswd) {
                        success = false
                        this.super_repasswd_msg = '两次输入的密码不一致，请检查'
                    }
                    if (success) {
                        this.setup = 3
                    }
                },
                toSetup4() {
                    this.clear()
                    let success = true
                    if (this.admin_username) {
                        if (this.admin_username.length < 3 || this.admin_username.length > 20) {
                            success = false
                            this.admin_username_msg = '管理员账号长度不能小于3或大于20'
                        }
                        if (!this.admin_password) {
                            success = false
                            this.admin_password_msg = '管理员密码未设置，请设置'
                        } else if (this.admin_password.length < 6 || this.admin_password.length > 20) {
                            success = false
                            this.admin_password_msg = '密码长度不能小于6或大于20'
                        }
                        if (!this.admin_repasswd) {
                            success = false
                            this.admin_repasswd_msg = '请再次输入账号登录密码'
                        } else if (this.admin_password != this.admin_repasswd) {
                            success = false
                            this.admin_repasswd_msg = '两次输入的密码不一致，请检查'
                        }
                    }
                    if (success) {
                        this.Install()
                        this.setup = 4
                    }
                },
                toFinish() {
                    location.href = '/install/finish'
                }
            }
        })
    </script>
}